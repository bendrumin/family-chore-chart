{
  "version": 2,
  "builds": [
    {
      "src": "frontend/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/frontend/api/health.js"
    },
    {
      "src": "/api/send-contact-email",
      "dest": "/frontend/api/send-contact-email.js"
    },
    {
      "src": "/api/create-paypal-checkout",
      "dest": "/frontend/api/create-paypal-checkout.js"
    },
    {
      "src": "/api/paypal-webhook",
      "dest": "/frontend/api/paypal-webhook.js"
    },
    {
      "src": "/api/test-webhook",
      "dest": "/frontend/api/test-webhook.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/frontend/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
} 